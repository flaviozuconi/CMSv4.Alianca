@{
    var portal = BLPortal.Atual;
    var baseUrl = "/portal/" + portal.Diretorio + "/";
}
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="canonical" href="URL-UNICA-DA-PAGINA" />
    <meta itemprop="name" content="NOME-DO-SITE - pode ser igual ao título da página" />
    <meta itemprop="description" content="Descrição entra aqui e é a mesma que estará na metatag DESCRIPTION." />
    <meta itemprop="image" content="http://www.nomedosite.com.br/includes/css/images/agencia-nomedosite.jpg" />
    <!-- Metatags gerais -->
    <meta name="description" content="A descrição entra aqui.">
    <title>Aliança Cabotagem</title>
    <meta name="keywords" content="a, list, of, keywords" />
    <!-- Metatags opcionais -->
    <meta name="copyright" content="copyright statement here" />
    <meta name="revisit-after" content="7 days" />
    <meta name="author" content="Nome do Autor" />
    <!-- Twitter Card -->
    <meta name="twitter:card" content="summary" />
    <meta name="twitter:site" content="flickr" />
    <meta name="twitter:title" content="Small Island Developing States Photo Submission" />
    <meta name="twitter:description" content="View the album on Flickr." />
    <meta name="twitter:image" content="https://farm6.staticflickr.com/5510/14338202952_93595258ff_z.jpg" />
    <meta name="twitter:url" content="https://www.flickr.com/photos/unicphoto/sets/72157645001703785/" />
    <!-- Facebook Open Graph -->
    <meta property="og:title" content="Aprenda a usar SEO para o Facebook, até 65 caracteres é o ideal" />
    <meta property="og:site_name" content="Nome do site" />
    <meta property="og:url" content="http://www.nomedosite.com.br/blog/posts/o-que-e-uma-agencia-digital--o-que-ela-pode-fazer-por-sua-empresa-/15" />
    <meta property="og:description" content="Uma descrição detalhada do conteúdo, o ideal é que seja 2 ou 4 sentenças, com uma limitação de caracteres até 160." />
    <meta property="og:image" content="http://www.nomedosite.com.br/files/nome-da-imagem.jpg" />
    <meta property="app_id" content="298758683566729" />
    <!-- tags opcionais -->
    <meta property="og:type" content="website" />
    <meta property="og:locale" content="pt_BR" />
    <meta property="og:locale:alternate" content="en_US" />
    <meta property="og:locale:alternate" content="es_ES" />
    <meta property="article:author" content="https://www.facebook.com/profile-ou-page-autor" />
    <meta property="article:publisher" content="https://www.facebook.com/cnn" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.0.0/animate.min.css" />
    <link rel="stylesheet" href="@Url.Content(baseUrl + "css/plugins.min.css")">
    <link rel="stylesheet" href="@Url.Content(baseUrl + "css/main.min.css")">

    <!-- fav icon / touch icon -->
    <link rel="apple-touch-icon" sizes="152x152" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#ffffff">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="theme-color" content="#ffffff">
    @RenderSection("head", false)
</head>
<body class="embarque-certo">

<!-- header -->
<header class="embarque-certo--header">
    <figure class="header--logo">
        <img src="@Url.Content(baseUrl + "img/alianca.png")" alt="">
    </figure>
</header>
<!-- /header -->

<!-- main -->
    @RenderSection("body", false)
    @RenderBody()
<!-- /main -->

 <!-- footer -->
<footer class="embarque-certo--footer">
    <figure class="footer--logo">
        <img src="img/alianca_footer.png" alt="">
    </figure>
    <ul class="footer--list">
        <li><strong>©2021 Aliança</strong></li>
        <li><a href="#">Maersk Company</a></li>
        <li><a href="#">Termos e condições</a> - <a href="#">Política de privacidade</a></li>
    </ul>
</footer>
<!-- /footer -->
    
<script src="@Url.Content(baseUrl + "js/jquery.min.js")"></script>
<script src="@Url.Content(baseUrl + "js/popper.min.js")"></script>
<script src="@Url.Content(baseUrl + "js/bootstrap.min.js")"></script>
<script src="@Url.Content(baseUrl + "js/plugins/owl.carousel.min.js")"></script>
<script src="@Url.Content(baseUrl + "js/plugins/bootstrap-select.min.js")"></script>
<script src="@Url.Content(baseUrl + "js/plugins/fullcalendar/moment.min.js")"></script>
<script src="@Url.Content(baseUrl + "js/plugins/fullcalendar/fullcalendar.min.js")"></script>
<script src="@Url.Content(baseUrl + "js/plugins/fullcalendar/pt-br.js")"></script>

<script src="@Url.Content(baseUrl + "js/calendar.js")"></script>
<script src="@Url.Content(baseUrl + "js/main.min.js")"></script>

@RenderSection("scripts", false)

</body>
</html>