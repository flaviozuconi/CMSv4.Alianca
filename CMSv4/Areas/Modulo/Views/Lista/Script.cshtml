@model MLModuloEventos
<script type="text/javascript">
    function fontSize(index) {
        if (index == 1) {
            $(".fontJQ").css("font-size", "large");
        }
        else if (index == 2) {
            $(".fontJQ").css("font-size", "x-large");
        }
        else {
            $(".fontJQ").css("font-size", "");
        }
    }
</script>