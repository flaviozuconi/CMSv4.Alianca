<div class="modal modal-confirmacao fade" id="modalPopupConfirmacao" tabindex="-1" aria-labelledby="modalPopupConfirmacao" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="modalConfirmacao"><strong>@T("Envio de solicitação")</strong></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <strong class="mensagemModal">@T("Deseja realmente enviar a solicitação?")</strong>
            </div>
            <div class="modal-footer">
                <a class="btn btn-success btnSimPopupConfirmacaoJQ">@T("Sim")</a>
                <a  class="btn btn-delete btnNaoPopupConfirmacaoJQ" data-dismiss="modal">@T("Não")</a>
            </div>
        </div>
    </div>
</div>