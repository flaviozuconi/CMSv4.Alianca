@section head {
    <link href="~/Content/css/plugins/bootstrap-tokenfield/bootstrap-tokenfield.css" rel="stylesheet" />
    <link href="~/Content/js/plugins/datetimepicker/jquery.datetimepicker.css" rel="stylesheet" />

    <link href="~/Content/css/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/css/jquery-ui.structure.css" rel="stylesheet" />
    <link href="~/Content/css/jquery-ui.theme.css" rel="stylesheet" />
}

@section scripts{

    <script src="~/Content/js/plugins/datetimepicker/jquery.datetimepicker.js"></script>
    <script src="~/Content/js/plugins/bootstrap-tokenfield/bootstrap-tokenfield.min.js"></script>
    <script src="~/Content/js/plugins/AceEditor/ace.js"></script>
    <script type="text/javascript">
        $('select[name=Categorias]').multiselect({
            includeSelectAllOption: true,
            nonSelectedText: '@TAdm("Nenhuma selecionada")'
        });

        $("#removercapa").on("click", function(){
            $("#imagemCapa").hide();
            $("#Imagem").val("");
            return false;
        });


        $('form').validate({
            rules: {
                Titulo: { required: true, maxlength: 250 },
                Url: {
                    maxlength: 250,
                    regex: "^[a-zA-Z][a-zA-Z0-9\-\_]+$" ,
                    remote: {
                        url: "/cms/@PortalAtual.Diretorio/ListaAdmin/ValidarUrl",
                        type: "post",
                        async: false,
                        loadingPanel: false,
                        data: {
                            id: function() {
                                return $('input[name=Codigo]').val()
                            },
                            Url: function(){
                                return $('input[name=Url]').val()
                            },
                            CodigoLista: "@config.Codigo"
                        }
                    }
                },
                Extra1: { maxlength: 250 },
                Extra2: { maxlength: 250 },
                Extra3: { maxlength: 250 },
                file: { extension: "png|jpg|jpeg|gif" }
                @if(adicionarValidacaoArquivo)
                {
                    @:,Max1: { extension: "png|jpg|jpeg|gif" }
                }
            },
            messages: {
                file: {
                    extension: "@TAdm("Extensão Inválida.")"
                }

                @if(adicionarValidacaoArquivo)
                {
                    @:,Max1: { extension: "@TAdm("Extensão Inválida.")" }
                }
            }
        });

        @(!AdminHelper.CheckPermission(Permissao.Modificar) ? "disableForm();" : "")

        $('input[name=Tags]').tokenfield();

        $("input[name=Data], input[name=DataPublicacao]").blur(function () {
            if ($(this).val() == "__/__/____ __:__") {
                $(this).val("");
            }
        });

        $('form').submit(function () {
            if ($(this).valid()) {
                if ($("input[name=Data]").val() == "__/__/____ __:__") {
                    $("input[name=Data]").val("");
                }

                if ($("input[name=DataPublicacao]").val() == "__/__/____ __:__") {
                    $("input[name=DataPublicacao]").val("");
                }
            }
        });

        $('input[name=Data]').datetimepicker({
            timepicker: true, mask: true, format: 'd/m/Y H:i', scrollMonth: true, scrollTime: true, scrollInput: false
        });
        $('input[name=DataPublicacao]').datetimepicker({
            timepicker: true, mask: true, format: 'd/m/Y H:i', scrollMonth: true, scrollTime: true, scrollInput: false
        });

        $('.jq-date-time').datetimepicker({
            timepicker: true, mask: true, format: 'd/m/Y H:i', scrollMonth: true, scrollTime: true, scrollInput: false
        });

        $('input[name=Titulo]').keyup(function () {
            var url = $('input[name=Url]');
            if (url) {
                if (url.data('editado')) return;

                url.val(UrlAmigavel($(this).val()));
            }
        });



        /*
        *   Tenta apresentar previamente a imagem antes de upload
        *   IE10 ou superior
        */
        function previewImage(input) {

            try {
                var reader = new FileReader();

                reader.onload = function (e) {
                    $('#imagemCapa').attr('src', e.target.result);
                }

                var files = input.files ? input.files : input.currentTarget.files;

                reader.readAsDataURL(files[0]);
            }
            catch (e) { }

        }

        function TemRevisar(str) {
            document.getElementById("IsRevisar").value = str;

        }
    </script>


    <script src="~/Content/js/jquery.sortable.min.js"></script>

    <!-- GALERIA -->


    <script type="text/javascript">

        $(document).ready(function () {
            /*
            *   FORM UPLOAD
            */
            Dropzone.options.galeriaUpload = {
                url: "@urlUploadGaleria",
                acceptedFiles: "image/*",
                dictDefaultMessage: "@TAdm("Arraste ou clique aqui para realizar upload de arquivos")",
                dictCancelUpload: "@TAdm("Cancelar upload")",
                dictCancelUploadConfirmation: "@TAdm("Cancelar upload")",
                dictMaxFilesExceeded: "@TAdm("Atingiu a quantidade de uploads permitidos")",
                dictFileTooBig: "@TAdm("Arquivo excedeu o tamanho permitido")",
                dictFallbackMessage: "@TAdm("Seu browser não suporta Drag and Drop")",
                sending: function (file, xhr, formData) {  formData.append("guid", '@(Model.GUID)'); },
                processing: function (file) { if (parent.sizeContent) parent.sizeContent(); },
                completemultiple: function (files) {
                    var arrayNames = [];

                    for (i = 0; i < files.length; i++) {
                        arrayNames.push(files[i].name);
                    }

                    ajaxIncluirImagem(arrayNames);
                }
            };

            /*
            * SORTABLE
            */
            $('#galeriaImagens').sortable();

        });

        function ajaxIncluirImagem(arrayNames) {
            $.ajax({
                type: "POST",
                url: "@urlIncluirGaleria",
                datatype: "json",
                traditional: true,
                data: {
                    guid: '@Model.GUID',
                    files: arrayNames
                },
                beforeSend: function () {
                    $("#spinner").show();
                },
                complete: function () {
                    Dropzone.forElement('#galeriaUpload').removeAllFiles(true);
                    $("#spinner").hide();
                },
                success: function (response) {
                    if (response.Sucesso != null && response.Sucesso) {
                        $('#galeriaImagens').append(response.html);
                    }
                },
                error: function(err){
                    console.log(err);
                }
            });
        }

        /*
        *   INCLUIR IMAGEM DO ARQUIVO EM DISCO PARA A GALERIA
        */
        function incluirImagem(botao) {

            var array = []
            var arquivo = botao.data('file');

            array.push(arquivo);

            ajaxIncluirImagem(array);
            botao.parent('li.galeria-imagem-item').remove();
        }

        /*
        *   REMOVER IMAGEM DA GALERIA (MANTER NO DISCO)
        */
        function removerImagem(botao, codigo, imagem) {

            $.post(
                '@urlRemoverGaleria', { guid: codigo, file: imagem },
                function (response) {
                    $(botao.parents('li.galeria-imagem-item')[0]).remove();
                    $('#galeriaImportar').append('<li class="form-group col-sm-2 galeria-imagem-item">' + response + '</li>');
                }
            );

        }

        /*
        *   REMOVER IMAGEM DA GALERIA (MANTER NO DISCO)
        */
        function excluirImagem(botao) {

            var imagem = botao.data('file');
            $.post(
                '@urlExcluirGaleria', { guid: '@Model.GUID', file: imagem },
                function (response) {
                    $(botao.parents('li.galeria-imagem-item')[0]).remove();
                }
            );

        }

    </script>

    <!-- VÍDEOS -->


    <script type="text/javascript">

        function ValiadarQtdeUploadVideos(){
            var videosCadastrados = $('ul#galeriaVideos li').length;

            if("@limiteVideos" > 0 && videosCadastrados > "@limiteVideos")
            {
                $("#videosUpload").hide();
                $("#videosAdd").hide();
            }
            else{
                $("#videosUpload").show();
                $("#videosAdd").show();
            }
        }

        $(document).ready(function () {
            ValiadarQtdeUploadVideos();

            /*
            *   FORM UPLOAD
            */
            Dropzone.options.videosUpload = {
                maxFileSize: @(limiteVideos == 0 ? 99999999 : limiteVideos ),
                url: "@urlUploadVideos",
                acceptedFiles: ".mp4",
                dictDefaultMessage: "@TAdm("Arraste ou clique aqui para realizar upload de arquivos")",
                dictCancelUpload: "@TAdm("Cancelar upload")",
                dictCancelUploadConfirmation: "@TAdm("Cancelar upload")",
                dictMaxFilesExceeded: "@TAdm("Atingiu a quantidade de uploads permitidos")",
                dictFileTooBig: "@TAdm("Arquivo excedeu o tamanho permitido")",
                dictFallbackMessage: "@TAdm("Seu browser não suporta Drag and Drop")",
                sending: function (file, xhr, formData) { formData.append("guid", '@(Model.GUID)'); },
                complete: function (file) {
                    var nome = file.name;

                    $.post(
                        '@urlIncluirVideos', { guid: '@Model.GUID', file: nome },

                        function (response) {
                            $('#galeriaVideos').append('<li class="form-group col-sm-12 galeria-video-item">' + response + '</li>');
                            $(file.previewElement).remove();
                            ValiadarQtdeUploadVideos();
                        }
                    );
                }
            };



            /*
            * SORTABLE
            */
            $('#galeriaVideos').sortable();
        });


        /*
        *   INCLUIR VÍDEO DO ARQUIVO EM DISCO PARA A GALERIA
        */
        function incluirVideo(botao) {

            var arquivo = botao.data('file');
            $.post('@urlIncluirVideos', { guid: '@Model.GUID', file: arquivo },
                function (response) {
                    botao.parent('li.galeria-video-item').remove();
                    $('#galeriaVideos').append('<li class="form-group col-sm-12 galeria-video-item">' + response + '</li>');
                }
            );
        }

        /*
        *   REMOVER VÍDEO DA GALERIA (MANTER NO DISCO)
        */
        function removerVideo(botao, codigo, imagem) {

            $.post(
                '@urlRemoverVideos', { guid: codigo, file: imagem },
                function (response) {
                    $(botao.parents('li.galeria-video-item')[0]).remove();
                    $('#videoImportar').append('<li class="form-group col-sm-12 galeria-video-item">' + response + '</li>');
                    ValiadarQtdeUploadVideos();
                }
            );

        }

        /*
        *   REMOVER VÍDEO DA GALERIA (MANTER NO DISCO)
        */
        function excluirVideo(botao) {
            ValiadarQtdeUploadVideos();
            var imagem = botao.data('file');
            $.post(
                '@urlExcluirVideos', { guid: '@Model.GUID', file: imagem },
                function (response) {
                    $(botao.parents('li.galeria-video-item')[0]).remove();
                }
            );
        }

        /*
        *   INCLUIR VÍDEO POR URL (YOUTUBE, VIMEO E ETC)
        */
        function incluirVideoUrl() {
            $.post('@urlIncluirUrlVideos', { guid: '@Model.GUID' },
            function (response) {
                contentHtml = $('#galeriaVideos').clone();
                $('#galeriaVideos').empty();
                $('#galeriaVideos').append('<li class="form-group col-sm-12 galeria-video-item">' + response + '</li>');
                $('#galeriaVideos').append(contentHtml);
                ValiadarQtdeUploadVideos();
            });
        }



    </script>

    <!-- AUDIO -->


    <script type="text/javascript">
        $(document).ready(function () {
            $("div#AudiosUpload").dropzone({
                maxFilesize: 1000,
                url: "@urlUploadAudios",
                acceptedFiles: ".mp3, .wma",
                dictDefaultMessage: "Upload",
                sending: function (file, xhr, formData) { formData.append("guid", '@(Model.GUID)'); },
                complete: function (file) {
                    var nome = file.name;

                    $.post(
                        '@urlIncluirAudios', { guid: '@Model.GUID', file: nome },
                        function (response) {
                            $('#galeriaAudios').append('<li class="form-group col-sm-12 galeria-Audio-item">' + response + '</li>');
                            $(file.previewElement).remove();
                        }
                    );
                }
            });

            $('#galeriaAudios').sortable();
        });


        /*
        *   INCLUIR VÍDEO DO ARQUIVO EM DISCO PARA A GALERIA
        */
        function incluirAudio(botao) {

            var arquivo = botao.data('file');
            $.post('@urlIncluirAudios', { guid: '@Model.GUID', file: arquivo },
            function (response) {
                botao.parent('li.galeria-Audio-item').remove();
                $('#galeriaAudios').append('<li class="form-group col-sm-12 galeria-Audio-item">' + response + '</li>');
            }
        );
        }

        /*
        *   REMOVER VÍDEO DA GALERIA (MANTER NO DISCO)
        */
        function removerAudio(botao, codigo, imagem) {

            $.post(
                '@urlRemoverAudios', { guid: codigo, file: imagem },
                function (response) {
                    $(botao.parents('li.galeria-Audio-item')[0]).remove();
                    $('#AudioImportar').append('<li class="form-group col-sm-12 galeria-Audio-item">' + response + '</li>');
                }
            );

        }

        /*
        *   REMOVER VÍDEO DA GALERIA (MANTER NO DISCO)
        */
        function excluirAudio(botao) {

            var imagem = botao.data('file');
            $.post(
                '@urlExcluirAudios', { guid: '@Model.GUID', file: imagem },
                function (response) {
                    $(botao.parents('li.galeria-Audio-item')[0]).remove();
                }
            );
        }

    </script>


