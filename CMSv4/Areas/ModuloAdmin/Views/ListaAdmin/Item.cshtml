@model  MLListaConteudo
@{
    var portal = PortalAtual.Diretorio;
    var usuario = BLUsuario.ObterLogado();
    var config = (MLListaConfig)ViewData["listaConfig"];
    var urlLista = "/cms/" + portal + "/listaadmin?idLista=" + config.Codigo;

    var json = Json.Decode(config.Configuracao);
    var categorias = (List<MLListaConfigCategoria>)ViewData["categorias-todas"] ?? new List<MLListaConfigCategoria>();
    var categoriasSelecionadas = (List<MLListaConteudoCategoria>)ViewData["categorias-selecionadas"] ?? new List<MLListaConteudoCategoria>();
    var aprovadores = (List<MLUsuario>)ViewData["aprovadores"] ?? new List<MLUsuario>();
    aprovadores.RemoveAll(o => o.Codigo == usuario.Codigo);

    var podeAprovar = ((usuario.Grupos.Find(o => o.CodigoGrupo == config.CodigoGrupoAprovador)) != null) || (aprovadores.Count == 0);


    if (!Model.GUID.HasValue) { Model.GUID = Guid.NewGuid(); }

    // GALERIA IMAGENS
    var lista = (List<MLListaConteudoImagem>)ViewData["lista"] ?? new List<MLListaConteudoImagem>();

    var urlUploadGaleria = "/cms/" + portal + "/listaadmin/uploadgaleria";
    var urlIncluirGaleria = "/cms/" + portal + "/listaadmin/incluirarquivogaleria";
    var urlRemoverGaleria = "/cms/" + portal + "/listaadmin/removerarquivogaleria";
    var urlExcluirGaleria = "/cms/" + portal + "/listaadmin/excluirarquivogaleria";

    //VIDEOS
    var listaVideo = (List<MLListaConteudoVideo>)ViewData["listaVideo"] ?? new List<MLListaConteudoVideo>();

    var urlUploadVideos = "/cms/" + portal + "/listaadmin/uploadvideo";
    var urlIncluirVideos = "/cms/" + portal + "/listaadmin/incluirarquivovideo";
    var urlRemoverVideos = "/cms/" + portal + "/listaadmin/removerarquivovideo";
    var urlExcluirVideos = "/cms/" + portal + "/listaadmin/excluirarquivovideo";
    var urlIncluirUrlVideos = "/cms/" + portal + "/listaadmin/incluirvideourl";

    //AUDIO
    var listaAudio = (List<MLListaConteudoAudio>)ViewData["listaAudio"] ?? new List<MLListaConteudoAudio>();

    var urlUploadAudios = "/cms/" + portal + "/listaadmin/UploadAudio";
    var urlIncluirAudios = "/cms/" + portal + "/listaadmin/incluirarquivoAudio";
    var urlRemoverAudios = "/cms/" + portal + "/listaadmin/removerarquivoAudio";
    var urlExcluirAudios = "/cms/" + portal + "/listaadmin/excluirarquivoAudio";

    var urlSeo = "/cms/" + portal + "/listaadmin/seooutros";

    var showGaleria = false;
    var showVideos = false;
    var showAudios = false;
    var showSEO = false;

    var adicionarValidacaoArquivo = false;

    int limiteVideos = 0;

    for (var i = 0; i < json.config.Length; i++)
    {
        if (json.config[i].campo == "Galeria") { showGaleria = true; }
        if (json.config[i].campo == "Videos")
        {
            showVideos = true;

            if (json.config[i].limite != null)
            {
                limiteVideos = Convert.ToInt16(json.config[i].limite);
            }
        }
        if (json.config[i].campo == "Audio") { showAudios = true; }
        if (json.config[i].campo == "SEO") { showSEO = true; }
    }
    var extra1IsSelect = false;
    var extra2IsSelect = false;
    var extra3IsSelect = false;


}


<div class="modal modal-flex fade " id="seoModal" tabindex="-1" role="dialog" aria-labelledby="flexModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">@TAdm("SEO Outros")</h4>
            </div>
            <div class="modal-body clearfix">
                <div class="form-group col-md-12">
                    <div class="form-group col-md-12">
                        <input type="hidden" name="CodigoLista" id="CodigoLista" value="@Model.Codigo" />
                        <textarea data-editor="html" id="SeoOutros" name="Seo.Outros" class="SeoOutros" style="height: 300px; width: 500px;">@Model.Seo.Outros</textarea>
                    </div>
                </div>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-white" data-dismiss="modal">@TAdm("Fechar")</button>
                <button type="button" onclick="return SalvarSeoOutros(@Model.Codigo);" class="btn btn-white">@TAdm("Salvar")</button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
</div>

<div class="portlet portlet-dark-blue">
    <div class="portlet-heading clearfix">
        <div class="portlet-title">
            <h4>
                <img src="@(config.Icone ?? "~/iconpack.axd/16/newspaper")" />@TAdm(config.Nome)
            </h4>
        </div>
        <div class="portlet-widgets">
            <a class="btn btn-orange" href="@urlLista">@TAdm("Voltar")</a>
        </div>
    </div>
    <div class="portlet-body clearfix">

        <ul class="nav nav-tabs">
            <li class="active"><a href="#cadastro" data-toggle="tab">@TAdm("Cadastro")</a></li>
            @if (showGaleria)
            {
                <li><a href="#galeria" data-toggle="tab"><span class="glyphicon glyphicon-camera"></span>@TAdm("Galeria")</a></li>
            }
            @if (showVideos)
            {
                <li><a href="#videos" data-toggle="tab"><span class="glyphicon glyphicon-film"></span>@TAdm("Vídeos")</a></li>
            }

            @if (showAudios)
            {
                <li><a href="#audios" data-toggle="tab"><span class="glyphicon glyphicon-headphones"></span>@TAdm("Áudio")</a></li>
            }

            @if (showSEO)
            {
                <li><a href="#seo" data-toggle="tab"><span class="glyphicon glyphicon-list-alt"></span>@TAdm("Seo - Redes Sociais")</a></li>
            }
        </ul>

        <form id="formCadastro" method="post" action="item" class="form-vertical tab-content" role="form" enctype="multipart/form-data" on>

            <div id="cadastro" class="tab-pane active">

                @if (!ViewData.ModelState.IsValid)
                {
                    @Html.ValidationSummary(false, "", new { @class = "alert alert-danger" });
                }

                <div class="form-group col-md-12">
                    <label>@TAdm("Idioma")</label>
                    @Helpers.ComboIdiomas(Model.CodigoIdioma, false)
                </div>

                @for (var i = 0; i < json.config.Length; i++)
                {
                    var campo = (string)json.config[i].campo;

                    switch (campo)
                    {
                        case "Titulo":
                            <div class="form-group @(json.config[i].css ?? "col-md-10")">
                                <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                <input type="text" class="form-control" name="Titulo" value="@Model.Titulo" maxlength="250" autofocus>
                            </div>
                            break;
                        case "Data":
                            <div class="form-group @(json.config[i].css ?? "col-md-2")">
                                <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                <input type="text" class="form-control" name="Data" maxlength="16" value="@( (Model.Data ?? DateTime.Now).ToString("dd/MM/yyyy HH:mm"))">
                            </div>
                            break;
                        case "Chamada":
                            <div class="form-group @(json.config[i].css ?? "col-md-12")">
                                <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                <textarea class="form-control" name="Chamada" style="height: 80px;">@Model.Chamada</textarea>
                            </div>
                            break;
                        case "Imagem":
                            <div class="form-group @(json.config[i].css ?? "col-md-3")">
                                <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                <input type="file" name="ImagemCapa" />
                                <input type="hidden" name="Imagem" id="Imagem" value="@Model.Imagem" />

                                @if (!string.IsNullOrEmpty(Model.Imagem))
                                {
                                    <img id="imagemCapa" src="@string.Concat(BLConfiguracao.Pastas.ModuloListas(portal), "/", Model.Codigo, "/capa", Model.Imagem)" alt="" style="max-width: 300px;" /><br />
                                    <input type="checkbox" id="RemoverCapa" name="RemoverCapa" value="True" /><label for="RemoverCapa">@TAdm("Remover")</label>
                                }
                            </div>

                            break;
                        case "Conteudo":

                            //string plugins = string.Empty;

                            //if (showGaleria)
                            //{
                            //    plugins += "galeria,";
                            //}

                            //if (showAudios)
                            //{
                            //    plugins += "audio,";
                            //}

                            //if (showVideos)
                            //{
                            //    plugins += "video,";
                            //}

                            <div class="form-group @(json.config[i].css ?? "col-md-12")">
                                <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                <textarea @*data-plugins="@plugins.TrimEnd(',')"*@ data-ckeditor="@portal" class="form-control" style="height: 300px;" name="Conteudo" id="conteudo">@Model.Conteudo</textarea>
                            </div>

                            break;

                        @*case "Max3":
                            if (json.config[i].tipo != null && !string.IsNullOrWhiteSpace(json.config[i].tipo) && json.config[i].tipo == "data")
                            {
                                <div class="form-group @(json.config[i].css ?? "col-md-2")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                    <input type="text" class="form-control jq-date-time" name="Max3" maxlength="16" value="@(!string.IsNullOrWhiteSpace(Model.Max3) ? Convert.ToDateTime(Model.Max3).ToString("dd/MM/yyyy HH:mm") : "")">
                                </div>
                            }
                            else
                            {
                                <div class="form-group @(json.config[i].css ?? "col-md-12")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                    <textarea class="form-control" name="Max3">@Model.Max3</textarea>
                                </div>
                            }
                            break;*@

                        case "Categoria":
                            bool multiplaEscolha = (bool?)json.config[i].multipla_escolha ?? true;

                            <div class="form-group @(json.config[i].css ?? "col-md-6")">
                                <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label><br />
                                <select name="Categorias" @(multiplaEscolha ? "multiple = \"multiple\"" : "")>
                                    @if (multiplaEscolha)
                                    {
                                        <option value="multiselect-all">@TAdm("Selecionar todos")</option>
                                    }
                                    @foreach (var categoria in categorias)
                                    {
                                        <option value="@categoria.Codigo" @(categoriasSelecionadas.Find(o => o.CodigoCategoria == categoria.Codigo) != null ? "selected" : "")>@categoria.Nome</option>
                                    }
                                </select>
                            </div>

                            break;
                        case "Tags":
                            <div class="form-group @(json.config[i].css ?? "col-md-6") tokenfield" data-value="blue">
                                <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                <input type="text" class="form-control" name="@json.config[i].campo" value="@Model.Tags">
                                <span class="help-block"><small>@TAdm("Pressione TAB para separar as Tags")</small></span>
                            </div>

                            break;
                        case "Url":
                            <div class="form-group @(json.config[i].css ?? "col-md-6")">
                                <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                <input type="text" class="form-control" name="Url" value="@Model.Url" maxlength="250">
                            </div>
                            break;
                        case "Extra1":
                            if (json.config[i].tipo != null && !string.IsNullOrWhiteSpace(json.config[i].tipo) && json.config[i].tipo == "checkbox")
                            {
                                <div class="form-group @(json.config[i].css ?? "col-md-3")">
                                    <input type="checkbox" name="Extra1" id="Extra1" value="true" @((!string.IsNullOrWhiteSpace(Model.Extra2) && Model.Extra1.ToLower() == "true") ? "checked" : "") />
                                    <label for="Extra1">@TAdm("Free")</label>
                                </div>
                            }
                            else if (json.config[i].tipo != null && !string.IsNullOrWhiteSpace(json.config[i].tipo) && json.config[i].tipo == "dropbox")
                            {
                                extra1IsSelect = true;
                                bool multiplaEscolhas = (bool?)json.config[i].multipla_escolha ?? true;
                                String valor = json.config[i].item;
                                String[] arrEscolha = valor.Split(',');
                                var listSelecionados = !string.IsNullOrEmpty(Model.Extra1) ? Model.Extra1.Split(',').ToList() : new List<string>();

                                <div class="form-group @(json.config[i].css ?? "col-md-3")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label><br />
                                    <select name="Extra1" @(multiplaEscolhas ? "multiple = \"multiple\"" : "")>
                                        @if (multiplaEscolhas)
                                        {
                                            <option value="multiselect-all">@TAdm("Selecionar todos")</option>
                                        }
                                        @foreach (var x in arrEscolha)
                                        {
                                            <option value="@(x)" @(listSelecionados.Find(o => o.Equals(x, StringComparison.InvariantCultureIgnoreCase)) != null ? "selected" : "")>@(x)</option>
                                        }
                                    </select>
                                </div>

                                break;
                            }
                            else
                            {
                                <div class="form-group @(json.config[i].css ?? "col-md-6")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                    <input type="text" class="form-control" name="Extra1" value="@Model.Extra1" maxlength="250">
                                </div>
                            }

                            break;
                        case "Extra2":
                            if (json.config[i].tipo != null && !string.IsNullOrWhiteSpace(json.config[i].tipo) && json.config[i].tipo == "checkbox")
                            {
                                <div class="form-group @(json.config[i].css ?? "col-md-3")">
                                    <input type="checkbox" name="Extra2" id="Extra2" value="true" @((!string.IsNullOrWhiteSpace(Model.Extra2) && Model.Extra2.ToLower() == "true") ? "checked" : "") />
                                    <label for="Extra2">@TAdm("Free")</label>
                                </div>
                            }
                            else if (json.config[i].tipo != null && !string.IsNullOrWhiteSpace(json.config[i].tipo) && json.config[i].tipo == "dropbox")
                            {
                                extra2IsSelect = true;
                                bool multiplaEscolhas = (bool?)json.config[i].multipla_escolha ?? true;
                                String valor = json.config[i].item;
                                String[] arrEscolha = valor.Split(',');
                                var listSelecionados = !string.IsNullOrEmpty(Model.Extra2) ? Model.Extra2.Split(',').ToList() : new List<string>();

                                <div class="form-group @(json.config[i].css ?? "col-md-3")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label><br />
                                    <select name="Extra2" @(multiplaEscolhas ? "multiple = \"multiple\"" : "")>
                                        @if (multiplaEscolhas)
                                        {
                                            <option value="multiselect-all">@TAdm("Selecionar todos")</option>
                                        }
                                        @foreach (var x in arrEscolha)
                                        {
                                            <option value="@(x)" @(listSelecionados.Find(o => o.Equals(x, StringComparison.InvariantCultureIgnoreCase)) != null ? "selected" : "")>@(x)</option>
                                        }
                                    </select>
                                </div>

                                break;
                            }
                            else
                            {
                                <div class="form-group @(json.config[i].css ?? "col-md-6")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                    <input type="text" class="form-control" name="Extra2" value="@Model.Extra2" maxlength="250">
                                </div>
                            }

                            break;
                        case "Extra3":
                            if (json.config[i].tipo != null && !string.IsNullOrWhiteSpace(json.config[i].tipo) && json.config[i].tipo == "checkbox")
                            {
                                <div class="form-group @(json.config[i].css ?? "col-md-3")">
                                    <input type="checkbox" name="Extra3" id="Extra3" value="true" @((!string.IsNullOrWhiteSpace(Model.Extra3) && Model.Extra3.ToLower() == "true") ? "checked" : "") />
                                    <label for="Extra3">@TAdm(json.config[i].rotulo)</label>
                                </div>
                            }
                            else if (json.config[i].tipo != null && !string.IsNullOrWhiteSpace(json.config[i].tipo) && json.config[i].tipo == "dropbox")
                            {
                                extra3IsSelect = true;
                                bool multiplaEscolhas = (bool?)json.config[i].multipla_escolha ?? true;
                                String valor = json.config[i].item;
                                String[] arrEscolha = valor.Split(',');

                                <div class="form-group @(json.config[i].css ?? "col-md-3")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label><br />
                                    <select name="Extra3" @(multiplaEscolhas ? "multiple = \"multiple\"" : "")>
                                        @if (multiplaEscolhas)
                                        {
                                            <option value="multiselect-all">@TAdm("Selecionar todos")</option>
                                        }
                                        @foreach (var x in arrEscolha)
                                        {
                                            <option value="@(x)" @(Model.Extra3 == x ? "selected" : "")>@(x)</option>
                                        }
                                    </select>
                                </div>

                                break;
                            }
                            else
                            {
                                <div class="form-group @(json.config[i].css ?? "col-md-6")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                    <input type="text" class="form-control" name="Extra3" value="@Model.Extra3" maxlength="250">
                                </div>
                            }
                            break;
                        case "Max1":
                            if (json.config[i].tipo != null && !string.IsNullOrWhiteSpace(json.config[i].tipo) && json.config[i].tipo == "data")
                            {
                                <div class="form-group @(json.config[i].css ?? "col-md-2")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                    <input type="text" class="form-control jq-date-time" name="Max1" maxlength="16" value="@(!string.IsNullOrWhiteSpace(Model.Max2) ? Convert.ToDateTime(Model.Max1).ToString("dd/MM/yyyy HH:mm") : "")">
                                </div>
                            }
                            else if (json.config[i].tipo != null && !string.IsNullOrWhiteSpace(json.config[i].tipo) && json.config[i].tipo == "file")
                            {
                                adicionarValidacaoArquivo = true;

                                <div class="form-group @(json.config[i].css ?? "col-md-6")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                    <input type="file" name="Max1Imagem">

                                    @if (!string.IsNullOrEmpty(Model.Max1))
                                    {
                                        <input type="hidden" name="Max1" id="Max1" value="@Model.Max1" />
                                        <img id="imagemThumb" src="@string.Concat(BLConfiguracao.Pastas.ModuloListas(portal), "/", Model.Codigo, "/thumb", System.IO.Path.GetExtension(Model.Max1))" alt="" style="max-width: 300px;" /><br />
                                    }
                                </div>
                            }
                            else
                            {
                                <div class="form-group @(json.config[i].css ?? "col-md-12")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                    <textarea class="form-control" name="Max1">@Model.Max1</textarea>
                                </div>
                            }


                            break;
                        case "Max2":
                            if (json.config[i].tipo != null && !string.IsNullOrWhiteSpace(json.config[i].tipo) && json.config[i].tipo == "data")
                            {
                                <div class="form-group @(json.config[i].css ?? "col-md-2")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                    <input type="text" class="form-control jq-date-time" name="Max2" maxlength="16" value="@(!string.IsNullOrWhiteSpace(Model.Max2) ? Convert.ToDateTime(Model.Max2).ToString("dd/MM/yyyy HH:mm") : "")">
                                </div>
                            }
                            else
                            {
                                <div class="form-group @(json.config[i].css ?? "col-md-12")">
                                    <label>@TAdm(json.config[i].rotulo ?? json.config[i].campo)</label>
                                    <textarea class="form-control" data-ckeditor="@portal" style="height: 300px;" name="Max2">@Model.Max2</textarea>
                                </div>
                            }
                            break;
                    }
                }

                <div class="col-md-6 col-md-offset-3">
                    @if (config.CodigoGrupoAprovador.HasValue)
                    {
                        <fieldset>
                            <legend>Aprovação</legend>
                            <div class="form-group col-md-12">

                                <input type="hidden" id="IsRevisar" name="IsRevisar" value="false" />

                                @if (podeAprovar)
                                {
                                    <input type="radio" id="Autorizar" name="Autorizar" value="@(!Model.CodigoUsuarioSolicitado.HasValue ? "true" : "false")" @(!Model.CodigoUsuarioSolicitado.HasValue ? "checked" : "") onclick="TemRevisar('false');" /> <label for="Autorizar">@TAdm("Autorizar conteúdo")</label><br />
                                }
                                @if (aprovadores.Count > 0)
                                {
                                    <input type="radio" id="Revisar" name="Autorizar" value="@(Model.CodigoUsuarioSolicitado.HasValue ? "true" : "false")" @(Model.CodigoUsuarioSolicitado.HasValue ? "checked" : "") onclick="TemRevisar('true');" /> <label for="Revisar">@TAdm("Solicitar aprovação")</label>
                                    <select name="CodigoUsuarioSolicitado" class="form-control">
                                        @foreach (var item in aprovadores)
                                        {
                                            <option value="@item.Codigo" @(item.Codigo == Model.CodigoUsuarioSolicitado ? "selected" : "")>@item.Nome (@item.Email)</option>
                                        }
                                    </select>
                                }
                                else
                                {
                                    <div class="alert alert-info">@TAdm("Não há outros aprovadores configurados.")</div>
                                }
                            </div>
                        </fieldset>
                    }

                    <fieldset>
                        <legend>@TAdm("Publicação")</legend>
                        <div class="form-group col-md-12">
                            <input type="radio" id="Publicar" name="Publicar" value="true" @(!Model.DataPublicacao.HasValue ? "checked='checked'" : "") />
                            <label for="Publicar">@TAdm("Publicar imediatamente")</label><br />
                            <input type="radio" id="Agendar" name="Publicar" value="false" @(Model.DataPublicacao.HasValue ? "checked='checked'" : "") />
                            <label for="Agendar">@TAdm("Agendar publicação")</label>
                            <input type="text" class="form-control" style="max-width: 150px;" name="DataPublicacao" maxlength="16" value="@(Model.DataPublicacao.HasValue ? Model.DataPublicacao.Value.ToString("dd/MM/yyyy HH:mm") : new DateTime(DateTime.Today.Year, DateTime.Today.Month, DateTime.Today.Day, 8, 0, 0).AddDays(1).ToString("dd/MM/yyyy HH:mm"))">
                        </div>
                    </fieldset>
                </div>

                <input type="hidden" value="@Model.Codigo" name="Codigo" />
                <input type="hidden" value="@Model.CodigoBase" name="CodigoBase" />
                <input type="hidden" value="@Model.GUID" name="GUID" id="GUID" />
                <input type="hidden" value="@config.Codigo" name="CodigoLista" />

                <div class="clearfix"></div>
                <div class="form-group col-md-3">
